    <frame group="1" frame="c5" name="fmrOpcion1" width="100%" height="100%" align="right|top" />
    <prop group="1" frame="fmrOpcion1" name="MapaActivos" contents="MapaActivos" width="99%" height="100%" title="MapaActivos" type="Z" visible="1" class="clsmapview" onmapclicked="onMapClicked(e);" onmapmovestarted="javascript:mapMoveStarted();" onmapmoveended="javascript:mapMoveEnded();" ondistancemeter="javascript:showDistance(e);" onlineclicked="javascript:segmentClicked(e);" />
