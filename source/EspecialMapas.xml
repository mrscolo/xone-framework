      <action name="runscript">
        <script language="javascript">
	      	appData.failWithMessage(-11888,"##EXIT##");
	      </script>
      </action>
    </onback>
    <before-edit refresh="true">
      <action name="runscript">
        <script language="javascript">			
			self.MAP_GROUP=1;
			self.MAP_TOTAL_PAGES=1;			
		</script>
      </action>
    </before-edit>
  </coll>
